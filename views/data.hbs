<br><br><br>
    <center>
    <h2>DATA PEMBAYARAN</h2>
    </center>
    <br><br>
     <table class="table table-hover">
        <thead>
            <tr class="bg-danger"> 
                <th>ID PRAKERJA</th>
                <th>NAMA</th>
                <th>TANGGAL LAHIR</th>
                <th>USIA</th>
                <th>ALAMAT</th>
                 <th>JENIS KELAMIN</th>
                <th>STATUS KERJA</th>
                <th>DELETE</th>
            </tr>
        </thead>
        <tbody class="table table-active">
            {{#each data}}
            <tr>
                <td>{{ ID_PEKERJA}}</td>
                <td>{{ NAMA }}</td>
                <td>{{ TANGGAL_LAHIR }}</td>
                <td>{{ USIA }}</td>
                <td>{{ ALAMAT }}</td>
                <td>{{ JENIS_KELAMIN }}</td>
                <td>{{ STATUS_KERJA }}</td>
                <td><a href="/hapus-ID_PEKERJA/{{ ID_PEKERJA }}"><button type="button" class="btn btn-danger">hapus</button></a></td>
            </tr>
            {{/each}}
        </tbody>
    </table>

<a href="/prakerja" class="btn btn-primary btn-lg active" role="button" aria-pressed="true">Primary link</a>

<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
